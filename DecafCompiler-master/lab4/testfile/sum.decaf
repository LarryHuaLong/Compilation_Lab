class Sum
{
    int getSum(int n)
    {
        int i;
        int sum;
        sum = 0;
        for (i = 1; i <= n; i = i + 1)
            sum = sum + i;
        return sum;
    }
}

class Main
{
    int main()
    {
        int n;
        int result;
        class Sum sum;
        sum = new Sum();
        Print("Input n:");
        n = ReadInteger();
        result = sum.getSum(n);
        Print("The sum is(display on digits):");
        Print(result);
        return 0;
    }
}
